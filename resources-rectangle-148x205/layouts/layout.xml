<layout id="MainLayout">
	<drawable id="TimeDisplay" class="TimeDisplay">
		<param name="adjustY">-95</param>
		<param name="timeFont">3</param><!-- Graphics.FONT_MEDIUM -->
	</drawable>

	<drawable id="SpeedMeter" class="BarMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="offsetX">0</param>
		<param name="offsetY">-70</param>
		<param name="strokeActive">20</param>
		<param name="strokeInactive">2</param>
		<param name="direction">:e</param>
		<param name="length">80</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">0</param>
		<param name="numberOffsetY">0</param>
		<param name="numberFont">8</param><!-- Graphics.FONT_NUMBER_THAI_HOT -->
		<param name="numberUnit">""</param>
	</drawable>

	<drawable class="TextField">
		<param name="text">"km/h"</param>
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="adjustX">0</param>
		<param name="adjustY">40</param>
		<param name="font">2</param>
	</drawable>

	<drawable id="BatteryMeter" class="BarMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="offsetX">-68</param>
		<param name="offsetY">0</param>
		<param name="strokeActive">12</param>
		<param name="strokeInactive">2</param>
		<param name="direction">:n</param>
		<param name="length">100</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">-30</param>
		<param name="numberOffsetY">75</param>
		<param name="numberFont">3</param><!-- Graphics.FONT_MEDIUM -->
		<param name="numberUnit">"%"</param>
	</drawable>

 	<drawable id="TemperatureMeter" class="BarMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="offsetX">68</param>
		<param name="offsetY">0</param>
		<param name="strokeActive">12</param>
		<param name="strokeInactive">2</param>
		<param name="direction">:n</param>
		<param name="length">100</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">30</param>
		<param name="numberOffsetY">75</param>
		<param name="numberFont">3</param><!-- Graphics.FONT_MEDIUM -->
		<param name="numberUnit">"c"</param>
	</drawable>

 	<drawable id="PowerMeter" class="BarMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="offsetX">50</param>
		<param name="offsetY">-25</param>
		<param name="strokeActive">4</param>
		<param name="strokeInactive">2</param>
		<param name="direction">:n</param>
		<param name="length">50</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">50</param>
		<param name="numberOffsetY">40</param>
		<param name="numberFont">0</param><!-- Graphics.FONT_XTINY -->
		<param name="numberUnit">"w"</param>
	</drawable>

 	<drawable id="IndicatorIcons" class="IndicatorIcons">
 		<param name="bluetoothOffsetX">-50</param>
 		<param name="bluetoothOffsetY">0</param>
 		<param name="batteryOffsetX">-60</param>
 		<param name="batteryOffsetY">75</param>
 		<param name="temperatureOffsetX">60</param>
 		<param name="temperatureOffsetY">75</param>
 		<param name="powerOffsetX">50</param>
 		<param name="powerOffsetY">20</param>
	</drawable>
	
</layout>
