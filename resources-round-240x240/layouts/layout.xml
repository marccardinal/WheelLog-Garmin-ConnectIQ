<layout id="MainLayout">
	<drawable id="TimeDisplay" class="TimeDisplay">
		<param name="adjustY">-70</param>
	</drawable>

	<drawable id="SpeedMeter" class="ArcMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="radius">108</param>
		<param name="strokeActive">20</param>
		<param name="strokeInactive">2</param>
		<param name="offsetDegrees">225</param>
		<param name="arcDegrees">270</param>
		<param name="direction">:clockwise</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">0</param>
		<param name="numberOffsetY">0</param>
		<param name="numberFont">8</param><!-- Graphics.FONT_NUMBER_THAI_HOT -->
		<param name="numberUnit">""</param>
	</drawable>

	<drawable class="TextField">
		<param name="text">"km/h"</param>
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="adjustX">0</param>
		<param name="adjustY">50</param>
		<param name="font">2</param>
	</drawable>

	<drawable id="BatteryMeter" class="ArcMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="radius">90</param>
		<param name="strokeActive">12</param>
		<param name="strokeInactive">2</param>
		<param name="offsetDegrees">225</param>
		<param name="arcDegrees">90</param>
		<param name="direction">:clockwise</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">-20</param>
		<param name="numberOffsetY">80</param>
		<param name="numberFont">0</param><!-- Graphics.FONT_XTINY -->
		<param name="numberUnit">"%"</param>
	</drawable>

 	<drawable id="TemperatureMeter" class="ArcMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="radius">90</param>
		<param name="strokeActive">12</param>
		<param name="strokeInactive">2</param>
		<param name="offsetDegrees">315</param>
		<param name="arcDegrees">90</param>
		<param name="direction">:counter_clockwise</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">20</param>
		<param name="numberOffsetY">80</param>
		<param name="numberFont">0</param><!-- Graphics.FONT_XTINY -->
		<param name="numberUnit">"c"</param>
	</drawable>

 	<drawable id="PowerMeter" class="ArcMeter">
		<param name="centerX">:center</param>
		<param name="centerY">:center</param>
		<param name="radius">75</param>
		<param name="strokeActive">4</param>
		<param name="strokeInactive">2</param>
		<param name="offsetDegrees">15</param>
		<param name="arcDegrees">30</param>
		<param name="direction">:counter_clockwise</param>
		<param name="numberDisplay">true</param>
		<param name="numberOffsetX">60</param>
		<param name="numberOffsetY">21</param>
		<param name="numberFont">0</param><!-- Graphics.FONT_XTINY -->
		<param name="numberUnit">"w"</param>
	</drawable>

 	<drawable id="IndicatorIcons" class="IndicatorIcons">
 		<param name="bluetoothOffsetX">-60</param>
 		<param name="bluetoothOffsetY">0</param>
 		<param name="batteryOffsetX">-50</param>
 		<param name="batteryOffsetY">80</param>
 		<param name="temperatureOffsetX">50</param>
 		<param name="temperatureOffsetY">80</param>
 		<param name="powerOffsetX">60</param>
 		<param name="powerOffsetY">0</param>
	</drawable>
	
</layout>
